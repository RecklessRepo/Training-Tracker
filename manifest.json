
{
  "manifest_version": 3,
  "name": "MarineNet Annual Training Tracker",
  "version": "1.1",
  "description": "Tracks required MarineNet training and expiration.",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["https://portal.marinenet.usmc.mil/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://portal.marinenet.usmc.mil/content/mnet-portal/en/my-completed-courses.html"],
      "js": ["content.js"]
    },
    {
      "matches": ["https://portal.marinenet.usmc.mil/content/mnet-portal/en/profile.html"],
      "js": ["profile.js"]
    }
  ]
}
